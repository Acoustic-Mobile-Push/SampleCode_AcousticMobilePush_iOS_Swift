source 'https://github.com/CocoaPods/Specs.git'

platform :ios, '12.1'
use_frameworks!

target 'Sample' do
  # If you want to use release binaries of AcousticMobilePush please change the following line
  # pod 'AcousticMobilePushDebug'
  # and use
  # pod 'AcousticMobilePush'
  pod 'AcousticMobilePushDebug'
end

